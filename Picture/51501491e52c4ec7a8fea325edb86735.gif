<script type="text/javascript">
	function sessions_icon(){
		setInterval(function(){
			$(".sessions_middle_praise_pic").removeClass("move_pic");
			setTimeout(function(){$(".sessions_middle_praise_pic").addClass("move_pic");},100);
		},2000);
	}
	sessions_icon();
	var sessions_num = $("#sessions_num").text();
	
	function intervalNums(){
		setInterval(function(){
			//获取点赞数值，并使用动画效果展示
			$.getJSON('http://html5-app.chinacloudapp.cn/api/?m=xsjsxsd2018&c=get&jsoncallback=?', function(n){
				var it = $(".new_era_num .nums");
				var len = String(n).length;
				for(var i=0; i<len; i++){
					if(it.length <= i){
						$(".new_era_num").append('<div class="nums">0123456789</div>');
					}
					var num = String(n).charAt(i);
					var t = parseInt(num,10)*20;
					$(".new_era_num .nums").eq(i).animate({"scrollTop":t+'px'}, 1000,'swing');
				}
			});
		},10000);
	}
	intervalNums();
	$(function(){
		function roll_fn(){
			var num1=1,num2=0,s=true,time=null,imgw=$('.roll_min_box li').width(),first=$('.roll_min_box li:first').clone(),last=$('.roll_min_box li:last').clone();
			$('.roll_main_box').scrollLeft(imgw);
			$('.roll_min_box').append(first);
			$('.roll_min_box').prepend(last);
			// 自动轮播
			function move(){
				time=setInterval(function (){
					num1++;
					if(num1>=$('.roll_min_box li').length){
						num1=2;
						$('.roll_main_box').scrollLeft(imgw);
					};
					num2++;
					if(num2>=$('.roll_icon li').length){
						num2=0;
					}
					$('.roll_icon li').eq(num2).addClass('roll_icon_color').siblings().removeClass('roll_icon_color');
					$('.roll_main_box').stop().animate({scrollLeft:imgw*num1},function (){
							s=true;
						})
				},5000)//5秒轮播一张
			};
			move();
			// 点击图标
			$('.roll_icon li').on("click",function(){
				clearInterval(time);
				num2=$('.roll_icon li').index(this);
				num1=num2+1;
				$('.roll_icon li').eq(num2).addClass('roll_icon_color').siblings().removeClass('roll_icon_color');
				$('.roll_main_box').stop().animate({scrollLeft:imgw*num1},function (){
						s=true;
					})
				move();
			})
		};
		roll_fn();
		function schedule_fn(){
			var time = null;
			$(".schedule_box").hover(function(){
				clearInterval(time);
			},function(){
				time = setInterval(function(){
					move();
				},2500)
			}).trigger("mouseleave");
			function move(){
				$('.schedule_box li:first').stop().animate({'margin-top':"-24px"},function(){
					$(this).css({'margin-top':"2px"}).appendTo($('.schedule_box ul'));					
				});
			};
			$(".prev").click(function(){
				$('.schedule_box li:last').prependTo($('.schedule_box ul'));
				$('.schedule_box li:first').css({'margin-top':"-24px"}).stop().animate({'margin-top':"2px"});
			});
			$(".next").click(function(){
				move();
			});
		}
		schedule_fn();
	})
</script>
